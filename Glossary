glossary = {
  'Python': 'High-level programming language.',
  'Key Value Pair': 'The method of organizing information in a python dictionary',
  'For loop': 'A function that loops through a series of commands',
  'Dictionary: 'Pythons built-in associative data type. A dictionary is made of key-value pairs where each key corresponds to a value.'
  }

print('This is my glossary:')
for key, value in glossary.items():
  print("\nKey: " + key)
  print('Value: ' + value)
	
